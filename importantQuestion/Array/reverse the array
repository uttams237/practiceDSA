# Write a program to reverse an array

def reverse(array):
    left,right=0,(len(array)-1)
    while left<right:
        array[left],array[right]=array[right],array[left]
        left+=1
        right-=1
    return


# main
while True:
    print("input the array : ",end="")
    array=input().split()
    print("original string is : ",array)
    reverse(array)
    print("reversed string is : ",array)
